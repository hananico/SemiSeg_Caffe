net: "SegNet/SiftFlow/segnet_train_siftflow_encode_decode_VGG.prototxt"  		# Change this to the absolute path to your model file
test_initialization: false
test_iter: 1
test_interval: 10000000
base_lr: 0.001
lr_policy: "step"
gamma: 0.1
stepsize: 5000
display: 5
momentum: 0.9
max_iter: 10000
weight_decay: 0.0005
snapshot: 100
snapshot_prefix: "SegNet/SiftFlow/segnet_basic_encode_decode_vgg"  	# Change this to the absolute path to where you wish to output solver snapshots
solver_mode: GPU
